{
  "name": "camfindlay/silverstripe-twofactorauth",
  "type": "silverstripe-module",
  "description": "Adds 2 factor authentication to SilverStripe CMS",
  "license": "BSD-3-Clause",
  "keywords": [
    "silverstripe",
    "2fa",
    "otp",
    "security",
    "authentication"
  ],
  "require": {
    "rych/otp": "1.1.*",
    "firesphere/bootstrapmfa": "^1.0@dev",
    "endroid/qr-code": "^2.0",
    "silverstripe/framework": "^3.3.2 || ^3.2.4 || ^3.1.19 || ^4.0.0"
  },
  "conflict": {
    "91carriage/2fa": "*"
  },
  "replace": {
    "91carriage/2fa": "*"
  },
  "authors": [
    {
      "name": "Simon Welsh",
      "email": "simon@simon.geek.nz",
      "homepage": "http://coding.simon.geek.nz/"
    },
    {
      "name": "Cam Findlay",
      "email": "cam@silverstripe.com"
    },
	{
        "name": "Fred Condo",
        "email": "fcondo@quinn.com",
        "role": "Developer",
        "homepage": "http://oddnoc.github.io"
    }
  ],
  "homepage": "https://github.com/camfindlay/silverstripe-twofactorauth",
  "support": {
    "issues": "https://github.com/camfindlay/silverstripe-twofactorauth/issues",
    "source": "https://github.com/camfindlay/silverstripe-twofactorauth/tree/master",
    "irc": "irc://irc.freenode.org/#silverstripe"
  },
  "autoload": {
      "psr-4": {
          "_2fa\\": "src/"
      }
  },
  "extra": {
    "installer-name": "twofactorauth",
    "screenshots": [
      "docs/en/_images/2fa-token-qr.png",
      "docs/en/_images/2fa-login.png",
      "docs/en/_images/2fa-backup.png"
    ],
    "branch-alias": {
      "dev-master": "3.x-dev"
    }
  }
}
